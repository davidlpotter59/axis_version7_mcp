  Type = "File";
  Domain = "SFPPRINT1";
  Padcharacter = Space;

  String L_FILE_NAME[16] = "mcp013_082010.cq";
  wdate l_eff_date;
  string l_company_id[10];
  unsigned ascii number l_state[2];
  unsigned ascii number l_1st_indent = 500;
  unsigned ascii number l_2nd_indent = 700;
  unsigned ascii number l_3rd_indent = 900;
